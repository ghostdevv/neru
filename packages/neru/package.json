{
    "name": "neru",
    "version": "1.0.1",
    "description": "Neru is a small and lightweight file router for your favourite backends",
    "author": "GHOST",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        }
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md",
        "CHANGELOG.md"
    ],
    "scripts": {
        "dev": "tsup-node --watch src",
        "build": "tsup-node",
        "test": "tsm node_modules/uvu/bin.js tests \".*\\.test\\.ts\""
    },
    "homepage": "https://github.com/ghostdevv/neru",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ghostdevv/neru"
    },
    "bugs": {
        "url": "https://github.com/ghostdevv/neru"
    },
    "dependencies": {
        "@nerujs/methods": "workspace:*",
        "consolite": "^0.3.11",
        "ghoststools": "^0.6.3",
        "kleur": "^4.1.5",
        "totalist": "^3.0.0"
    },
    "devDependencies": {
        "uvu": "^0.5.6"
    }
}
